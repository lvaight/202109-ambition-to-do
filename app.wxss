/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 13% 0;
  box-sizing: border-box;
} 

.button {
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  margin: 50rpx 50rpx auto;
  font-size: small;
  border-radius: 10rpx;
  background-color: white;
  border: 5rpx solid #E7624F;
}

